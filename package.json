{
  "name": "goujila-md-editor",
  "version": "0.0.3",
  "description": "Open source editor for writing math formulas",
  "main": "lib/index.js",
  "scripts": {
    "simple": "./node_modules/.bin/webpack-dev-server --inline --config ./sample/webpack.config.js",
    "compile": "./node_modules/.bin/babel -d lib/ -D src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/777720/GoujilaMDEditor.git"
  },
  "author": "geek720",
  "license": "MIT",
  "dependencies": {
    "github-markdown-css": "^2.10.0",
    "lodash": "^4.17.5",
    "markdown-it": "^8.4.1",
    "markdown-it-katex": "^2.0.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.1.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "babel": {
    "presets": [
      [
        "env"
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ]
  }
}
